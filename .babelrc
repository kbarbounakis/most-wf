{
  "sourceMap": true,
  "presets": [ "env" ],
  "ignore": "node_modules",
  "plugins": [
    ["transform-es2015-modules-commonjs-simple", {
      "noMangle":true,
      "addExports":true
    }],
    "transform-es2015-typeof-symbol",
    "transform-decorators-legacy",
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error", "Array"]
    }]
  ]
}